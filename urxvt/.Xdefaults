Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintfull

urxvt*depth: 32
urxvt*background: rgba:0000/0000/0200/c100

URxvt.depth: 32
URxvt.loginShell: true
URxvt.saveLines: 20000
URxvt.internalBorder: 3
URxvt.lineSpace: 0
URxvt*background:   #000000
URxvt*foreground:   #d0d0d0

URxvt*cursorColor:  #d0d0d0

URxvt*colorBD:      #3579a8
URxvt*colorUL:      #4eb4fa

!black
URxvt*color0:       #000000
URxvt*color8:       #1c1c1c

!red
URxvt*color1:       #960050
URxvt*color9:       #ff0090

!green
URxvt*color2:       #66aa11
URxvt*color10:      #80ff00

!yellow
URxvt*color3:       #c47f2c
URxvt*color11:      #ffba68

!blue
URxvt*color4:       #30309b
URxvt*color12:      #5f5fee

!magenta
URxvt*color5:       #7e40a5
URxvt*color13:      #bb88dd

!cyan
URxvt*color6:       #3579a8
URxvt*color14:      #4eb4fa

!white
URxvt*color7:       #5f5f5f
URxvt*color15:      #d0d0d0


! Normal
*URxvt*keysym.Home:		\033OH
*URxvt*keysym.End:		\033OF
*URxvt*keysym.F1:		\033OP
*URxvt*keysym.F2:		\033OQ
*URxvt*keysym.F3:		\033OR
*URxvt*keysym.F4:		\033OS

*URxvt*keysym.M-Up:		\033[1;3A
*URxvt*keysym.M-Down:   \033[1;3B
*URxvt*keysym.M-Right:  \033[1;3C
*URxvt*keysym.M-Left:   \033[1;3D
URxvt*font: xft:Source Code Pro:size=12:antialias=true:hinting=true,xft:WenQuanYi Micro Hei Mono:pixelsize=18:minspace=true
xterm*locale:       true
xterm.cjkWidth:     true
urxvt.letterSpace: -1

URxvt.perl-ext-common: ...,clipboard,...
! clipboard
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
! 禁用到Ctrl+Shift的输入特殊字符功能
URxvt.iso14755: False
