Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintfull

urxvt*depth: 32
urxvt*background: rgba:0000/0000/0200/c100

URxvt.depth: 32
URxvt.loginShell: true
URxvt.saveLines: 20000
URxvt.internalBorder: 3
URxvt.lineSpace: 0
 
URxvt*background:   #000000
URxvt*foreground:   #d0d0d0

URxvt*cursorColor:  #d0d0d0

URxvt*colorBD:      #3579a8
URxvt*colorUL:      #4eb4fa

!black
URxvt*color0:       #000000
URxvt*color8:       #1c1c1c

!red
URxvt*color1:       #960050
URxvt*color9:       #ff0090

!green
URxvt*color2:       #66aa11
URxvt*color10:      #80ff00

!yellow
URxvt*color3:       #c47f2c
URxvt*color11:      #ffba68

!blue
URxvt*color4:       #30309b
URxvt*color12:      #5f5fee

!magenta
URxvt*color5:       #7e40a5
URxvt*color13:      #bb88dd

!cyan
URxvt*color6:       #3579a8
URxvt*color14:      #4eb4fa

!white
URxvt*color7:       #5f5f5f
URxvt*color15:      #d0d0d0
 
URxvt*font: xft:DejaVu Sans Mono:size=14:antialias=true:hinting=true,xft:WenQuanYi Micro Hei Mono:pixelsize=18:minspace=true
xterm*locale:       true
xterm.cjkWidth:     true
urxvt.letterSpace: -1

URxvt.perl-ext-common: ...,clipboard,...
! clipboard
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
! 禁用到Ctrl+Shift的输入特殊字符功能
URxvt*iso14755_52: false
